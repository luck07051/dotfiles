# Reload sxhkd
super + ctrl + shift + r
    killall sxhkd; sxhkd

# {Raise, Low} volume
super + shift + z
  notify-send 'pacman -Syuu completed!' 'this is content, brabra'

super + F12
  slock

#===== AUDIO =====#
# {Raise, Low} volume
super + shift + {u,i}
    vl {up,down} 5

{XF86AudioRaiseVolume, XF86AudioLowerVolume}
    vl {up,down} 5

{button10, button11}
    vl {up,down} 5


# switch output
super + shift + o
    vl switch


# Mute output
XF86AudioMute
    pactl set-source-mute @DEFAULT_SINK@ toggle; \
# Mute source
XF86AudioMicMute
    pactl set-source-mute @DEFAULT_SOURCE@ toggle



# Screenshot
Print
    flameshot gui


super + o
    menu_bookmark
