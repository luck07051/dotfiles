# Reload sxhkd
super + ctrl + shift + r
    killall sxhkd; sxhkd


#===== AUDIO =====#
# {Raise, Low} volume
super + shift + {u,i}
    volume {volUp,Down} 5

{XF86AudioRaiseVolume, XF86AudioLowerVolume}
    volume vol{Up,Down} 5

{button10, button11}
    volume vol{Up,Down} 5


# switch output
super + shift + o
    volume nextSink


# Mute output
XF86AudioMute
    pactl set-source-mute @DEFAULT_SINK@ toggle; \
# Mute source
XF86AudioMicMute
    pactl set-source-mute @DEFAULT_SOURCE@ toggle



# Screenshot
Print
    flameshot gui
